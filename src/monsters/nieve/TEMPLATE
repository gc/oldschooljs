import LootTable from '../../structures/LootTable';
import SimpleMonster from '../../structures/SimpleMonster';
import HerbDropTable from '../subtables/HerbDropTable';
import RareDropTable, { GemTable } from '../subtables/RareDropTable';
import RareSeedTable from '../subtables/RareSeedTable';

export const Table = new LootTable()
    .every('Bones')
    .oneIn(, '')
    .oneIn(, '')

    /*Weapons and armour */
    .addItem('', , )
    .addItem('', , )
    .addItem('', , )
    .addItem('', , )
    .addItem('', , )
    .addItem('', , )
    .addItem('', , )
    .addItem('', , )
    .addItem('', , )
    .addItem('', , )

    /* Runes and ammunition */
    .addItem('', , )
    .addItem('', , )
    .addItem('', , )
    .addItem('', , )
    .addItem('', , )

    /* Herbs */
    .add(HerbDropTable, undefined, 3)

    /* Seeds */
    .addItem(RareSeedTable, undefined, 1)
    /* Materials */
    .addItem('', , )
    .addItem('', , )
    .addItem('', , )
    .addItem('', , )
    .addItem('', , )

    /* Coins */
    .addItem('Coins', , )
    .addItem('Coins', , )
    .addItem('Coins', , )
    .addItem('Coins', , )

    /* Other */
    .addItem('', , )
    .addItem('', , )
    .addItem('', , )
    .addItem('', , )
    .addItem('', , )

    /* Rdt */
    .add(RareDropTable, undefined, 1)
    .add(GemTable, undefined, 2)

    /* Tertiary */
    .tertiary(,'')
    .tertiary(,'')
    .tertiary(,'')
    .tertiary(,'')

    /* Catacombs tertiary */
    .tertiary(, 'Ancient shard')
    .tertiary(, 'Dark totem base')
    .tertiary(, 'Dark totem middle')
    .tertiary(, 'Dark totem top');

export default new SimpleMonster({ id: , name: '', table: Table });
